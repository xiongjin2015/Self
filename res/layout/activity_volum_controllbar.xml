<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:xj="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#55ffffff"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <com.xj.self.view.lmj623565791.CustomVolumControllBar
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        xj:bg="@drawable/customize"
        xj:circleWidth="12dp"
        xj:dotCount="20"
        xj:firstColor="#252420"
        xj:secondColor="#FDFDFD"
        xj:splitSize="25" />

    <com.xj.self.view.lmj623565791.CustomVolumControllBar
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        xj:bg="@drawable/icon"
        xj:circleWidth="10dp"
        xj:dotCount="10"
        xj:firstColor="#00ff00"
        xj:secondColor="#ff0000"
        xj:splitSize="15" />

    <com.xj.self.view.lmj623565791.CustomVolumControllBar
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="10dp"
        xj:bg="@drawable/icon_qq_small"
        xj:circleWidth="10dp"
        xj:dotCount="8"
        xj:firstColor="#ADC6E5"
        xj:secondColor="#0362DB"
        xj:splitSize="25" />

</LinearLayout>